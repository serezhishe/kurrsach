{
  "name": "kurs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --no-progress && npm run build-server",
    "build:ci": "ng build --no-progress --prod && npm run build-server:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-server": "tsc -p tsconfig.backend.json --sourceMap true",
    "build-server:prod": "tsc -p tsconfig.backend.json --sourceMap false",
    "watch-server": "tsc -w -p tsconfig.backend.json --sourceMap true",
	"open": "npm i && ng build --no-progress && npm run build-server && nodemon dist/app.js"
  },
  "private": true,
